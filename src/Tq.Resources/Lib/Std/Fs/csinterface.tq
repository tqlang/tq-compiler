
@dotnetImport("System.Runtime", "System.IO.FileStream")
struct CSharp_FileStream {

	@dotnetImport constructor(string path, CSharp_FileMode fileMode, CSharp_FileAccess fileAccess) *CSharp_FileStream

	@dotnetImport @public func Flush() void
	@dotnetImport @public func Flush(bool flushToDisk) void
	@dotnetImport @public func Seek(i64 offset, CSharp_SeekOrigin origin) i64
	
	@dotnetImport("System.Runtime", "System.IO.Stream", "Close") @public func Close() void
}

@dotnetImport("System.Runtime", "System.IO.SeekOrigin")
typedef CSharp_SeekOrigin {
	Begin,
	Current,
	End,
}

@dotnetImport("System.Runtime", "System.IO.FileMode")
typedef CSharp_FileMode {
	CreateNew,
	Create,
	Open,
	OpenOrCreate,
	Truncate,
	Append,
}

@dotnetImport("System.Runtime", "System.IO.FileAccess")
typedef CSharp_FileAccess {
	Read,
	Write,
	ReadWrite,
}
